{
    "name": "nette2/project",
    "require": {
        "composer-synchronizer/composer-synchronizer": "@dev",
        "machy8/webloader": "1.2.0"
    },
    "extra": {
        "composer-synchronizer": {
            "project-type": "nette",
            "paths-placeholders": {
                "tempDir": "temporaryFiles"
            }
        }
    },
    "repositories": [
        {"type": "path", "url": "packages/some/package"}
    ],
    "minimal-stability": "dev",
    "prefer-stable": true
}
